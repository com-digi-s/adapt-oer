  "instruction": "<script>var mutationCallback=function(mutationsList,observer){for(var mutation of mutationsList){if(mutation.type==='childList'){var wrapper=document.getElementById('wrapper');if(wrapper&&(wrapper.children.length>0||wrapper.querySelector('.page'))){observer.disconnect();setupIntersectionObserver();break}}}};function setupIntersectionObserver(){var elements=document.querySelectorAll('.component');if(elements.length>=2){var targetElement=elements[elements.length-2];var intersectionCallback=function(entries){entries.forEach(function(entry){var arrowElement=document.querySelector('.abs__btn-arrow.is-right');if(entry.isIntersecting){arrowElement.classList.add('last-next')}else{arrowElement.classList.remove('last-next')}})};var observer=new IntersectionObserver(intersectionCallback);observer.observe(targetElement)}}var observer=new MutationObserver(mutationCallback);observer.observe(document.getElementById('wrapper'),{childList:true,subtree:true});</script>",
